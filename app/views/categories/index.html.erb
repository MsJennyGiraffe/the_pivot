<div class="fluid-container">
  <h1 class="text-center">All Categories</h1>
</div>

<div class="container">
<% @categories.each do |category| %>
  <div class="col-xs-12 col-md-4">
    <div class="text-center btn btn-default btn-block"> <%= link_to category.title, category_path(category.title) %> </div>
  </div>
<% end %>
</div>
