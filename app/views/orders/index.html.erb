<div class="well col-md-11">
  <h1 class="text-center">Your Orders</h1>
  <div>
    <div class="container col-md-11">
      <table class="table table-bordered table-striped table-responsive col-md">
        <th>Order Number</th>
        <th></th>
        <% @orders.each do |order| %>
        <tr>
          <td><%= order.id %> </td>
          <td><%= link_to "View Order", order_path(order) %></td>
        </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
