<div class="well">
  <h1 class="text-center">All Orders</h1>
  <div>
    <div class="container">
      <table class="table table-bordered table-striped">
        <th>Order Number</th>
        <th></th>
        <% @orders.each do |order| %>
        <tr>
          <td><%= order.id %> </td>
          <td><%= link_to "View Order", order_path(order) %></td>
        </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
