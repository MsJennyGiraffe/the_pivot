<div class="panel panel-primary">
  <h1> Create New Account</h1>
</div>

<div class="panel panel-primary">
  <%=  form_for(@user) do |f| %>
  <%= f.label :username, "Username" %>
  <%= f.text_field :username %>

  <%= f.label :email, "Email" %>
  <%= f.text_field :email %>

  <%= f.label :password, "Password" %>
  <%= f.password_field :password %>

  <%= f.submit "Create Account" %>

<% end %>
</div>
