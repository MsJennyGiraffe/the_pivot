<h1>Select a Pick Up Time</h1>

<ul><strong>Pick Up Hours:</strong>
  <li>Tuesday - Saturday</li>
  <li>10:00am - 3:00pm</li>
</ul>
<br>
<%= form_for([@order, @reservation]) do |f| %>
  <%= f.label :pickup_time %>
  <%= f.datetime_select :pickup_time %>
  <%= f.submit "Make Reservation" %>
  <%#= f.select :pickup_time, [ ["10am", 10], ["11am", 11], ["12pm", 12], ["1pm", 13], ["2pm", 14], ["3pm", 15] ] %>
<% end %>
